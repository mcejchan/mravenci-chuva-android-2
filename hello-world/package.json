{
  "name": "hello-world",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:devclient": "../expert-development-build-monitored.sh",
    "run:devclient": "env ADB_SERVER_SOCKET=tcp:host.docker.internal:5037 adb -s emulator-5554 reverse tcp:8081 tcp:8081 && env ADB_SERVER_SOCKET=tcp:host.docker.internal:5037 adb -s emulator-5554 shell am start -a android.intent.action.VIEW -d \"helloworld://expo-development-client/?url=http%3A%2F%2Flocalhost%3A8081\"",
    "build:bundled": "../expert-bundled-build-monitored.sh",
    "install:qa": "../qa-install.sh",
    "monitor": "../process-monitor.sh"
  },
  "dependencies": {
    "expo": "~53.0.0",
    "expo-build-properties": "~0.14.8",
    "expo-dev-client": "~5.2.4",
    "expo-dev-launcher": "5.1.16",
    "expo-modules-core": "~2.5.0",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.17",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "expo-asset": "~11.1.7"
  },
