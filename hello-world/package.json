{
  "name": "hello-world",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild:qa": "../qa-prebuild.sh",
    "build:qa": "../qa-build.sh",
    "build:bundled": "npm run prebuild:qa && npm run build:qa",
    "install:qa": "../qa-install.sh",
    "monitor": "../process-monitor.sh",
    "test:appium": "jest tests/appium --testTimeout=30000",
    "test:appium:debug": "ADB_SERVER_SOCKET=tcp:host.docker.internal:5037 jest tests/appium --testTimeout=30000",
    "test:appium:watch": "jest tests/appium --watch --testTimeout=30000",
    "test:full-cycle": "npm run build:bundled && npm run install:qa && npm run test:appium:debug",
    "appium:start": "ADB_SERVER_SOCKET=tcp:host.docker.internal:5037 npx appium server -p 4723 --log-level debug",
    "appium:verify": "nc -vz host.docker.internal 5037 && adb devices -H host.docker.internal -P 5037"
  },
  "dependencies": {
    "expo": "~53.0.0",
    "expo-asset": "~11.1.7",
    "expo-build-properties": "~0.14.8",
    "expo-modules-core": "~2.5.0",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.17",
    "react": "19.0.0",
    "react-native": "0.79.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/cli": "^0.24.21",
    "appium": "^3.0.2",
    "appium-uiautomator2-driver": "^5.0.3",
    "jest": "^29.7.0",
    "webdriverio": "^7.40.0"
  },
  "private": true
}
